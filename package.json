{
  "name": "fdc-modular-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Founders Direct Cockpit - Modular Starter (Next.js 16 + React 19)",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:visual": "playwright test tests/e2e/visual.spec.ts",
    "test:visual:update": "playwright test tests/e2e/visual.spec.ts --update-snapshots",
    "audit:check": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "audit:fix:verify": "npm audit fix && npm run type-check && npm run test:unit",
    "outdated": "npm outdated",
    "deps:check": "npm run outdated && npm run audit:check"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "lucide-react": "^0.556.0",
    "next": "16.0.10",
    "pino": "^10.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stripe": "^20.3.1",
    "swr": "^2.4.0",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.10.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "jsdom": "^28.1.0",
    "pino-pretty": "^13.1.3",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=22.22.0"
  }
}
